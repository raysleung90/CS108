            <string>///Player Input &amp; Collision Detection
//Read player input.
//Based on tutorial code from Shaun Spalding.
key_right = keyboard_check(vk_right);
key_left = -keyboard_check(vk_left);
key_jump = keyboard_check_pressed(vk_up);

//React to player input.
move = key_left + key_right; //Move left if only left is pressed and vice versa.
horizSpd = move * moveSpd;
if(vertSpd &lt; 15) vertSpd += grav;

if(place_meeting(x,y + 1, objGroundPlcHldr)) //Check collision with floor.
{
    if(key_jump) vertSpd = -jumpSpd;
}

var horizSpd_final = horizSpd + horizSpd_carry;
horizSpd_carry = 0;

//Horizontal collision detection.
if(place_meeting(x + horizSpd_final, y, objGroundPlcHldr))
{
    while(!place_meeting(x + sign(horizSpd_final), y, objGroundPlcHldr))
    {
        x += sign(horizSpd_final);
    }
    horizSpd_final = 0;
    horizSpd = 0;
}

x += horizSpd_final;

//Vertical collision detection.
if(place_meeting(x, y + vertSpd, objGroundPlcHldr))
{
    while(!place_meeting(x, y + sign(vertSpd), objGroundPlcHldr))
    {
        y += sign(vertSpd);
    }
    vertSpd = 0;
}
y += vertSpd;


</string>
